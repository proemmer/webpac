{
  "version": "0.1.0-beta1",
  "description": "Core server components for ASP.NET SignalR.",
  "dependencies": {
    "Microsoft.AspNetCore.SignalR.Sources": {
      "version": "0.1.0-beta1",
      "type": "build"
    },
    "Microsoft.AspNetCore.SignalR.Messaging": "0.1.0-beta1",
    "Microsoft.AspNetCore.DataProtection": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Hosting.Abstractions": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Http.Extensions": "1.0.0-rc2-final",
    "Microsoft.Extensions.DependencyModel": "1.0.0-rc2-final",
    "Newtonsoft.Json": "8.0.3"
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "allowUnsafe": true,
    "keyFile": "../../tools/Key.snk",
    "embed": {
      "include": "Scripts/**/*.*"
    }
  },
  "frameworks": {
    "netstandard1.5": {
      "dependencies": {
        "System.Runtime.Serialization.Primitives": "4.1.1-*"
      },
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ]
    }
  }
}